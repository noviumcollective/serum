{
  "plugins": [
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6":     true,
      "node":    true,
      "mocha":   true
    },   
    "extends": [
      "eslint:recommended"
    ],    
    "rules": {
      "no-console": 0
    }
}
